struct Fuga {
    x: Int
    y: Double
    flag: Bool
}

fn main(): Int {
    foo = 2 + 3 * ((5 - 1) + 1) / 3
    bar: Double = 9
    flag = bar < 5 - 1

    s = <Fuga x={foo} y={bar} flag={flag} />
    return <baz a={s} b={8} />
}

fn baz(a: Fuga, b: Double): Int {
    if a.flag { return 0 }
    if a.y > b { return 1 }

    return a.x
}
